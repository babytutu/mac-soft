{
  "git.confirmSync": false,
  "workbench.colorTheme": "One Dark Pro",
  "editor.fontSize": 14,
  "files.autoSave": "onFocusChange",
  "editor.tabSize": 2,
  "window.openFilesInNewWindow": "on",
  "window.openFoldersInNewWindow": "on",
  "emmet.syntaxProfiles": {
    "vue-html": "html",
    "vue": "html"
  },
  "beautify.language": {
    "js": {
      "type": [
        "javascript",
        "json"
      ],
      "filename": [
        ".jshintrc",
        ".jsbeautify"
      ]
    },
    "css": [
      "css",
      "scss",
      "stylus"
    ],
    "html": [
      "htm",
      "html",
      "vue"
    ]
  },
  "beautify.config": {
    "html": {
      "allowed_file_extensions": ["htm", "html", "xhtml", "shtml", "xml", "svg", "vue"],
      "brace_style": "collapse",
      "end_with_newline": false,
      "indent_char": " ",
      "indent_handlebars": false,
      "indent_inner_html": false,
      "indent_scripts": "keep",
      "indent_size": 2,
      "eol": "\n",
      "max_preserve_newlines": 0,
      "wrap_attributes": "force-aligned",
      "preserve_newlines": true,
      "unformatted": ["a", "span", "img", "code", "pre", "sub", "sup", "em", "strong", "b", "i", "u", "strike", "big", "small", "pre", "h1", "h2", "h3", "h4", "h5", "h6", "style"],
      "wrap_line_length": 0,
      "space_around_combinator": true,
      "jslint_happy": true,
    },
    "css": {
      "allowed_file_extensions": ["css", "scss", "sass", "less", "stylus"],
      "end_with_newline": false,
      "indent_char": " ",
      "indent_size": 2,
      "eol": "\n",
      "newline_between_rules": true,
      "selector_separator": " ",
      "selector_separator_newline": true
    },
    "js": {
      "allowed_file_extensions": ["js", "jsx", "json", "jshintrc", "jsbeautifyrc"],
      "brace_style": "collapse,preserve-inline",
      "break_chained_methods": false,
      "e4x": true,
      "end_with_newline": false,
      "indent_char": " ",
      "indent_level": 0,
      "indent_size": 2,
      "eol": "\n",
      "indent_with_tabs": false,
      "jslint_happy": false,
      "keep_array_indentation": false,
      "keep_function_indentation": false,
      "max_preserve_newlines": 0,
      "preserve_newlines": true,
      "space_after_anon_function": true,
      "space_before_conditional": true,
      "space_in_empty_paren": false,
      "space_in_paren": false,
      "unescape_strings": false,
      "wrap_line_length": 0
    }
  },
  "editor.renderWhitespace": "boundary",
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/.git": true
  },
  "gitProjectManager.baseProjectsFolders": [
    "/Users/tutu/Downloads/code"
  ],
  "workbench.iconTheme": "vscode-icons",
  "gitProjectManager.maxDepthRecursion": 1,
  "workbench.colorCustomizations": {
    "activityBar.background": "#212121",
    "sideBar.background": "#212121",
    "statusBar.background": "#212121",
    "titleBar.activeBackground": "#212121",
    "editor.background": "#212121",
    "editorGroup.background": "#212121",
    "tab.activeBackground": "#212121",
    "tab.border": "#212121",
    "tab.inactiveBackground": "#333",
    "editor.selectionBackground": "#eee"
  },
  "editor.wordWrap": "on",
  "files.trimTrailingWhitespace": true,
  "vsicons.dontShowNewVersionMessage": true,
  "terminal.integrated.shell.osx": "/bin/zsh",
  "gitlens.advanced.messages": {
    "suppressCommitHasNoPreviousCommitWarning": false,
    "suppressCommitNotFoundWarning": false,
    "suppressFileNotUnderSourceControlWarning": false,
    "suppressGitVersionWarning": false,
    "suppressLineUncommittedWarning": false,
    "suppressNoRepositoryWarning": false,
    "suppressResultsExplorerNotice": false,
    "suppressShowKeyBindingsNotice": true,
    "suppressUpdateNotice": false,
    "suppressWelcomeNotice": true
  },
  "git.autofetch": true,
  "workbench.startupEditor": "newUntitledFile",
  "gitlens.keymap": "alternate",
  "explorer.confirmDelete": false,
  "git.enableSmartCommit": true,
  "files.associations": {
    "*.wpy": "vue"
  },
  "path-intellisense.mappings": {
    "/": "${workspaceRoot}",
    "src": "${workspaceRoot}/src",
  }
}